    <TEXTBLOCK CONF="0.220">
      <LINE TYPE="本文" X="1546" Y="709" WIDTH="44" HEIGHT="1232" CONF="0.884" ORDER="18" STRING="なむあみだ すゞろにいひてさしぐみぬ。 見ま" />
      <LINE TYPE="本文" X="1461" Y="708" WIDTH="44" HEIGHT="690" CONF="0.780" ORDER="19" STRING="はす木立ち もの音もなき" />
      <LINE TYPE="本文" X="1285" Y="707" WIDTH="41" HEIGHT="1231" CONF="0.883" ORDER="23" STRING="ざぶ〓〓と、 をり〓〓水は岸をうつ。 ひとりさ" />
      <LINE TYPE="本文" X="1200" Y="707" WIDTH="43" HEIGHT="575" CONF="0.823" ORDER="24" STRING="びしく 麥蹈みてゐむ" />
      <LINE TYPE="本文" X="1020" Y="704" WIDTH="43" HEIGHT="1236" CONF="0.843" ORDER="28" STRING="白じろと たゞむき出し畝をうつ 畠の男 あ" />
      <LINE TYPE="本文" X="935" Y="709" WIDTH="41" HEIGHT="393" CONF="0.818" ORDER="29" STRING="ち向きて 久し" />
      <LINE TYPE="本文" X="756" Y="703" WIDTH="44" HEIGHT="1231" CONF="0.864" ORDER="32" STRING="日の光り そびらにあびて寒く行く百姓をとこ。" />
