    <TEXTBLOCK CONF="0.666">
      <LINE TYPE="本文" X="2773" Y="791" WIDTH="54" HEIGHT="572" CONF="0.762" ORDER="0" STRING="え て、 かげろふ日の 面" />
      <LINE TYPE="本文" X="2844" Y="672" WIDTH="69" HEIGHT="1342" CONF="0.810" ORDER="5" STRING="裾野原 野の上に遠き人の行き いつまでも見" />
      <SHAPE>
        <POLYGON POINTS="2773,672,2773,2013,2914,2013,2914,672" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.263">
      <LINE TYPE="本文" X="2578" Y="782" WIDTH="84" HEIGHT="1221" CONF="0.768" ORDER="8" STRING="諸縣の山にすぐなる柚の道。 疑はなくに は" />
      <LINE TYPE="本文" X="2519" Y="778" WIDTH="42" HEIGHT="213" CONF="0.713" ORDER="10" STRING="夕づけり" />
      <LINE TYPE="本文" X="2316" Y="770" WIDTH="94" HEIGHT="1248" CONF="0.801" ORDER="14" STRING="山下に、 屋庭まひろきひと構へ。 道はおりたり。" />
      <LINE TYPE="本文" X="2254" Y="770" WIDTH="44" HEIGHT="275" CONF="0.782" ORDER="15" STRING="その夕庭に" />
      <LINE TYPE="本文" X="2053" Y="761" WIDTH="72" HEIGHT="1226" CONF="0.877" ORDER="18" STRING="山の子は、 後姿さびしも。 風呂たきて、 手拭白" />
      <LINE TYPE="本文" X="1990" Y="760" WIDTH="47" HEIGHT="512" CONF="0.808" ORDER="19" STRING="く かづきたりけり" />
      <SHAPE>
        <POLYGON POINTS="1991,738,1991,2016,2916,2016,2916,738" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.227">
      <LINE TYPE="本文" X="1495" Y="764" WIDTH="73" HEIGHT="1221" CONF="0.912" ORDER="21" STRING="この家の人の ゆふげにまじりつゝ、 もの言ひ" />
      <LINE TYPE="本文" X="1410" Y="767" WIDTH="50" HEIGHT="514" CONF="0.850" ORDER="22" STRING="ことなる我と思へり" />
      <LINE TYPE="本文" X="1232" Y="765" WIDTH="75" HEIGHT="1226" CONF="0.918" ORDER="24" STRING="旅ごゝろのおどろき易きを叱りつゝ 柴火のく" />
      <LINE TYPE="本文" X="1146" Y="771" WIDTH="53" HEIGHT="571" CONF="0.837" ORDER="25" STRING="づれ 立てなほし居り" />
      <LINE TYPE="本文" X="971" Y="776" WIDTH="75" HEIGHT="1220" CONF="0.911" ORDER="28" STRING="日のゝちを いきれ殘れり。 茶臼原の夏うぐひ" />
      <LINE TYPE="本文" X="883" Y="779" WIDTH="52" HEIGHT="510" CONF="0.872" ORDER="30" STRING="すは、 草ごもり鳴く" />
      <LINE TYPE="本文" X="708" Y="784" WIDTH="72" HEIGHT="1218" CONF="0.908" ORDER="31" STRING="こすもすの蕾かたきに、 手觸りたり。 旅をやめ" />
      <LINE TYPE="本文" X="617" Y="666" WIDTH="54" HEIGHT="568" CONF="0.759" ORDER="33" STRING="なむ 心を持ちて 111" />
      <SHAPE>
        <POLYGON POINTS="621,749,621,2006,1570,2006,1570,749" />
      </SHAPE>
    </TEXTBLOCK>
