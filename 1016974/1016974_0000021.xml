    <TEXTBLOCK CONF="0.454">
      <LINE TYPE="本文" X="2484" Y="786" WIDTH="46" HEIGHT="1220" CONF="0.889" ORDER="4" STRING="うちわたす 大茅原となりにけり。 茅の葉光る" />
      <LINE TYPE="本文" X="2406" Y="785" WIDTH="41" HEIGHT="275" CONF="0.865" ORDER="5" STRING="暑き風かも" />
      <LINE TYPE="本文" X="2223" Y="781" WIDTH="50" HEIGHT="1236" CONF="0.891" ORDER="6" STRING="鳥の聲 遙かなるかも。 山腹の午後の日ざしは、" />
      <LINE TYPE="本文" X="2147" Y="781" WIDTH="42" HEIGHT="333" CONF="0.880" ORDER="7" STRING="旅を倦ましむ" />
      <SHAPE>
        <POLYGON POINTS="2144,776,2144,2019,2528,2019,2528,776" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.868">
      <LINE TYPE="本文" X="1963" Y="772" WIDTH="46" HEIGHT="1223" CONF="0.863" ORDER="8" STRING="高く來て、 音なき霧のうごき見つ。 木むらにひ" />
      <LINE TYPE="本文" X="1883" Y="782" WIDTH="42" HEIGHT="564" CONF="0.884" ORDER="9" STRING="ゞく われのしはぶき" />
      <SHAPE>
        <POLYGON POINTS="1885,777,1885,1997,2009,1997,2009,777" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.785">
      <LINE TYPE="本文" X="1548" Y="732" WIDTH="78" HEIGHT="1223" CONF="0.917" ORDER="10" STRING="篶深き山澤遠き見おろしに、 轆轤音して、 家ち" />
      <LINE TYPE="本文" X="1468" Y="741" WIDTH="42" HEIGHT="269" CONF="0.835" ORDER="11" STRING="ひさくあり" />
      <SHAPE>
        <POLYGON POINTS="1471,732,1471,1956,1621,1956,1621,732" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.854">
      <LINE TYPE="本文" X="1289" Y="741" WIDTH="77" HEIGHT="1219" CONF="0.929" ORDER="14" STRING="澤なかの木地屋の家にゆくわれの ひそけき步" />
      <LINE TYPE="本文" X="1207" Y="745" WIDTH="51" HEIGHT="508" CONF="0.872" ORDER="15" STRING="みは 誰知らめやも" />
      <SHAPE>
        <POLYGON POINTS="1208,741,1208,1961,1361,1961,1361,741" />
