      <LINE TYPE="本文" X="2542" Y="715" WIDTH="47" HEIGHT="1222" CONF="0.891" ORDER="5" STRING="金太郞よ 起きねと 夜はによびたれば、 湯に" />
      <LINE TYPE="本文" X="2458" Y="720" WIDTH="44" HEIGHT="803" CONF="0.856" ORDER="6" STRING="や行かすと ねむりつゝ聞けり" />
      <LINE TYPE="本文" X="2280" Y="715" WIDTH="51" HEIGHT="1221" CONF="0.907" ORDER="8" STRING="人こぞる湯ぶねの上のがすの燈を 年かはる時" />
      <LINE TYPE="本文" X="2193" Y="716" WIDTH="53" HEIGHT="446" CONF="0.598" ORDER="9" STRING="と 瞻りつゝ居り" />
      <LINE TYPE="本文" X="2018" Y="707" WIDTH="50" HEIGHT="1225" CONF="0.884" ORDER="13" STRING="湯のそとに、 はなしつゝ洗ふ人の聲 げに 事" />
