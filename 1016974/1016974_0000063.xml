  <PAGE IMAGENAME="1016974_R0000063.jp2" WIDTH="3621" HEIGHT="2575">
    <TEXTBLOCK CONF="0.632">
      <LINE TYPE="本文" X="2797" Y="638" WIDTH="83" HEIGHT="1277" CONF="0.771" ORDER="0" STRING="谷風に 花のみだれのほの〓〓し。 靑野の槿 112" />
      <LINE TYPE="本文" X="2737" Y="747" WIDTH="47" HEIGHT="331" CONF="0.755" ORDER="1" STRING="山の邊に散る" />
      <SHAPE>
        <POLYGON POINTS="2740,634,2740,1917,2878,1917,2878,634" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.757">
      <LINE TYPE="本文" X="2533" Y="739" WIDTH="71" HEIGHT="1223" CONF="0.890" ORDER="4" STRING="燒けはらの石ふみわたるわがうへに、 山の夕雲" />
      <LINE TYPE="本文" X="2467" Y="803" WIDTH="51" HEIGHT="391" CONF="0.798" ORDER="5" STRING="ひくゝ垂れ來も" />
      <SHAPE>
        <POLYGON POINTS="2468,740,2468,1964,2602,1964,2602,740" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.467">
      <LINE TYPE="本文" X="2268" Y="728" WIDTH="70" HEIGHT="1227" CONF="0.876" ORDER="7" STRING="ゆふだちの雨みだれ來る茅原ゆ、 むかつ丘かけ" />
      <LINE TYPE="本文" X="2202" Y="737" WIDTH="47" HEIGHT="447" CONF="0.832" ORDER="8" STRING="道見えわたる て" />
      <LINE TYPE="本文" X="2026" Y="723" WIDTH="68" HEIGHT="1226" CONF="0.854" ORDER="11" STRING="野のをちを つらなりとほる馬のあし つばら" />
      <LINE TYPE="本文" X="1959" Y="730" WIDTH="52" HEIGHT="506" CONF="0.848" ORDER="13" STRING="に動く。 夕雲の下に" />
      <SHAPE>
        <POLYGON POINTS="1965,722,1965,1958,2336,1958,2336,722" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.260">
      <LINE TYPE="本文" X="1497" Y="742" WIDTH="65" HEIGHT="1222" CONF="0.915" ORDER="14" STRING="幹だちのおぼめく木々に、 ゆふべの雨 さやぐ" />
      <LINE TYPE="本文" X="1415" Y="745" WIDTH="52" HEIGHT="633" CONF="0.886" ORDER="15" STRING="を聞きて、 とまりに急ぐ" />
      <LINE TYPE="本文" X="1234" Y="749" WIDTH="65" HEIGHT="1237" CONF="0.912" ORDER="17" STRING="麥かちて 人らいこへる庭なかの 榎のうれに、" />
      <LINE TYPE="本文" X="1151" Y="751" WIDTH="46" HEIGHT="333" CONF="0.842" ORDER="19" STRING="鳥あまた動く" />
      <LINE TYPE="本文" X="973" Y="752" WIDTH="61" HEIGHT="1218" CONF="0.890" ORDER="20" STRING="庭の木に、 ひまなくうごく鳥のあたま 見つゝ" />
      <LINE TYPE="本文" X="889" Y="814" WIDTH="50" HEIGHT="570" CONF="0.849" ORDER="21" STRING="遠ゆくことを忘れ居り" />
      <LINE TYPE="本文" X="710" Y="756" WIDTH="64" HEIGHT="1220" CONF="0.897" ORDER="24" STRING="竝み木原、 車井のあと をちこち見ゆ。 國は古" />
      <LINE TYPE="本文" X="624" Y="638" WIDTH="52" HEIGHT="631" CONF="0.837" ORDER="26" STRING="國。 家居さだまらず 113" />
      <SHAPE>
        <POLYGON POINTS="630,667,630,1988,1564,1988,1564,667" />
      </SHAPE>
    </TEXTBLOCK>
