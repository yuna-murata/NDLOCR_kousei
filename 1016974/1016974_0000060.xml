    <TEXTBLOCK CONF="0.579">
      <LINE TYPE="本文" X="2851" Y="682" WIDTH="75" HEIGHT="1339" CONF="0.793" ORDER="0" STRING="言たえて 久しくなりぬ。 始羅の山 喘へつゝ 106" />
      <LINE TYPE="本文" X="2785" Y="800" WIDTH="51" HEIGHT="626" CONF="0.844" ORDER="2" STRING="越ゆと 知らずやあらむ" />
      <SHAPE>
        <POLYGON POINTS="2785,678,2785,2020,2923,2020,2923,678" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.561">
      <LINE TYPE="本文" X="2590" Y="794" WIDTH="66" HEIGHT="1221" CONF="0.907" ORDER="4" STRING="日の照りの おとろへそむる野の土の あつき" />
      <LINE TYPE="本文" X="2522" Y="788" WIDTH="50" HEIGHT="574" CONF="0.852" ORDER="5" STRING="乾きを 草鞋にふむも" />
      <SHAPE>
        <POLYGON POINTS="2520,786,2520,2016,2654,2016,2654,786" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.430">
      <LINE TYPE="本文" X="2331" Y="784" WIDTH="63" HEIGHT="1221" CONF="0.913" ORDER="7" STRING="火の峰の山ふところに 寢て居りと思ふこゝろ" />
      <LINE TYPE="本文" X="2260" Y="784" WIDTH="44" HEIGHT="509" CONF="0.840" ORDER="8" STRING="おどろかめやも は" />
      <LINE TYPE="本文" X="2067" Y="777" WIDTH="63" HEIGHT="1221" CONF="0.897" ORDER="10" STRING="木々とよむ雨のなかより 鳥の聲 けたゝまし" />
      <LINE TYPE="本文" X="1995" Y="777" WIDTH="49" HEIGHT="629" CONF="0.872" ORDER="11" STRING="くして、 やみにけるかも" />
      <SHAPE>
        <POLYGON POINTS="1996,772,1996,2004,2392,2004,2392,772" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.224">
      <LINE TYPE="本文" X="1489" Y="762" WIDTH="81" HEIGHT="1224" CONF="0.497" ORDER="13" STRING="兒湯の山 棚田の奥に、 妹と 夫と 飯はむ家" />
      <LINE TYPE="本文" X="1410" Y="763" WIDTH="46" HEIGHT="450" CONF="0.780" ORDER="15" STRING="を 我は見にけり" />
      <LINE TYPE="本文" X="1231" Y="775" WIDTH="78" HEIGHT="1214" CONF="0.888" ORDER="18" STRING="つばらに さゝ波光る赤江灘。 この峰のうへゆ" />
      <LINE TYPE="本文" X="1148" Y="834" WIDTH="50" HEIGHT="333" CONF="0.820" ORDER="19" STRING="見窮めがたし" />
      <LINE TYPE="本文" X="968" Y="773" WIDTH="78" HEIGHT="1178" CONF="0.915" ORDER="21" STRING="海風の吹き頻く丘の砂の窪。 散りたまる葉は、" />
      <LINE TYPE="本文" X="889" Y="779" WIDTH="48" HEIGHT="334" CONF="0.809" ORDER="22" STRING="すべて靑き葉" />
      <SHAPE>
        <POLYGON POINTS="627,722,627,2022,1578,2022,1578,722" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.762">
      <LINE TYPE="本文" X="708" Y="780" WIDTH="78" HEIGHT="1239" CONF="0.902" ORDER="26" STRING="木のもとの仰ぎ に 疎き枝のうれ。 朝間の空は、" />
      <LINE TYPE="本文" X="629" Y="669" WIDTH="50" HEIGHT="448" CONF="0.808" ORDER="27" STRING="色かはり易し 107" />
