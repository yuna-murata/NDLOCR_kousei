    <TEXTBLOCK CONF="0.395">
      <LINE TYPE="本文" X="2841" Y="628" WIDTH="67" HEIGHT="1345" CONF="0.830" ORDER="0" STRING="かの見ゆる丘根の篶原 ひたくだりに、 さ夜風 64" />
      <LINE TYPE="本文" X="2768" Y="753" WIDTH="46" HEIGHT="629" CONF="0.815" ORDER="1" STRING="おだやむ 月夜のひゞき" />
      <LINE TYPE="本文" X="2577" Y="744" WIDTH="64" HEIGHT="1216" CONF="0.848" ORDER="4" STRING="をちかたに、 水霧ひ照る湍のあかり 龍女のか リユウニヨ" />
      <LINE TYPE="本文" X="2508" Y="743" WIDTH="47" HEIGHT="508" CONF="0.798" ORDER="5" STRING="群れつゝをどる げ" />
      <LINE TYPE="本文" X="2316" Y="736" WIDTH="65" HEIGHT="1216" CONF="0.865" ORDER="9" STRING="光る湍の 其處につどはす三世の佛 まじらひ" />
      <LINE TYPE="本文" X="2241" Y="735" WIDTH="59" HEIGHT="511" CONF="0.764" ORDER="10" STRING="がたき現身。 われは" />
      <LINE TYPE="本文" X="2052" Y="732" WIDTH="66" HEIGHT="1158" CONF="0.860" ORDER="12" STRING="ひたぶるに月夜おし照る河原かも。 立たすは" />
      <LINE TYPE="本文" X="1975" Y="725" WIDTH="59" HEIGHT="632" CONF="0.789" ORDER="13" STRING="藥師。 坐るは 釋迦文尼 モ" />
      <SHAPE>
        <POLYGON POINTS="1989,591,1989,1971,2905,1971,2905,591" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.259">
      <LINE TYPE="本文" X="1551" Y="756" WIDTH="73" HEIGHT="1213" CONF="0.909" ORDER="15" STRING="湍を過ぎて、 淵によどめる波のおも。 かそけき" />
      <LINE TYPE="本文" X="1467" Y="756" WIDTH="55" HEIGHT="570" CONF="0.804" ORDER="16" STRING="音も なくなりにけり" />
      <LINE TYPE="本文" X="1289" Y="751" WIDTH="75" HEIGHT="1227" CONF="0.870" ORDER="19" STRING="時ありて 渦波おこる淵のおも。 何おともなき" />
      <LINE TYPE="本文" X="1207" Y="820" WIDTH="48" HEIGHT="392" CONF="0.859" ORDER="20" STRING="そのめぐりはも" />
      <LINE TYPE="本文" X="1028" Y="760" WIDTH="76" HEIGHT="1222" CONF="0.862" ORDER="23" STRING="うづ波のもなか 穿けたり。 見る〓〓に 靑蓮" />
      <LINE TYPE="本文" X="945" Y="759" WIDTH="56" HEIGHT="632" CONF="0.849" ORDER="24" STRING="華のはな 咲き出づらし" />
      <SHAPE>
        <POLYGON POINTS="682,719,682,1996,1624,1996,1624,719" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.762">
      <LINE TYPE="本文" X="767" Y="765" WIDTH="73" HEIGHT="1222" CONF="0.882" ORDER="30" STRING="水底に、 うつそみの面わ 沈透き見ゆ。 來む世" />
      <LINE TYPE="本文" X="676" Y="645" WIDTH="61" HEIGHT="749" CONF="0.856" ORDER="27" STRING="も、 我の 寂しくあらむ 65" />
      <SHAPE>
        <POLYGON POINTS="680,646,680,1987,845,1987,845,646" />
      </SHAPE>
    </TEXTBLOCK>
