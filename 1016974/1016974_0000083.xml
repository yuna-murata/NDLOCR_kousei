    <TEXTBLOCK CONF="0.261">
      <LINE TYPE="本文" X="1482" Y="723" WIDTH="76" HEIGHT="1214" CONF="0.893" ORDER="15" STRING="朝あつき村を來はなれ、 道なかに、 汗をふきつ" />
      <LINE TYPE="本文" X="1399" Y="730" WIDTH="45" HEIGHT="503" CONF="0.867" ORDER="16" STRING="ものゝさびしさ" />
      <LINE TYPE="タイトル本文" X="1205" Y="971" WIDTH="49" HEIGHT="232" CONF="0.741" ORDER="19" STRING="名 濱" />
      <LINE TYPE="本文" X="1021" Y="728" WIDTH="77" HEIGHT="1221" CONF="0.919" ORDER="20" STRING="晝あつき家にこもれば、 濱風の まさごはあが" />
      <LINE TYPE="本文" X="936" Y="734" WIDTH="50" HEIGHT="448" CONF="0.878" ORDER="21" STRING="る。 竹の簀の子に" />
      <LINE TYPE="本文" X="757" Y="734" WIDTH="74" HEIGHT="1221" CONF="0.913" ORDER="23" STRING="夕かげの まほなるものか。 をちかたに 洲崎" />
