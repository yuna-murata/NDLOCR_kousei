  <PAGE IMAGENAME="1016974_R0000056.jp2" WIDTH="3621" HEIGHT="2575">
    <TEXTBLOCK CONF="0.707">
      <LINE TYPE="本文" X="2836" Y="643" WIDTH="61" HEIGHT="1347" CONF="0.860" ORDER="0" STRING="尾張ノ少昨のぼらず。 年滿ちて、 きのふも 今 日 98" />
      <LINE TYPE="本文" X="2758" Y="769" WIDTH="48" HEIGHT="448" CONF="0.765" ORDER="1" STRING="も、 人續ぎて上る" />
      <SHAPE>
        <POLYGON POINTS="2764,643,2764,1990,2895,1990,2895,643" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.763">
      <LINE TYPE="本文" X="2573" Y="769" WIDTH="57" HEIGHT="1215" CONF="0.840" ORDER="4" STRING="つくしの遊行孃子になづみつゝ、 旅人は タビト 竟 ツヒ に" />
      <LINE TYPE="本文" X="2499" Y="764" WIDTH="42" HEIGHT="336" CONF="0.838" ORDER="5" STRING="還りたりけり" />
      <SHAPE>
        <POLYGON POINTS="2500,763,2500,1986,2637,1986,2637,763" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.864">
      <LINE TYPE="本文" X="2308" Y="763" WIDTH="52" HEIGHT="1225" CONF="0.893" ORDER="7" STRING="よき司 われは持たらぬ憶良ゆゑ、 汝がゐやま" />
      <LINE TYPE="本文" X="2228" Y="765" WIDTH="47" HEIGHT="628" CONF="0.884" ORDER="8" STRING="ひは、 受け得ずなりたり" />
      <SHAPE>
        <POLYGON POINTS="2232,762,2232,1989,2359,1989,2359,762" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.724">
      <LINE TYPE="本文" X="2043" Y="997" WIDTH="52" HEIGHT="450" CONF="0.667" ORDER="10" STRING="かの少咋の爲に" />
      <SHAPE>
        <POLYGON POINTS="2043,997,2043,1447,2095,1447,2095,997" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.344">
      <LINE TYPE="本文" X="1531" Y="756" WIDTH="64" HEIGHT="1221" CONF="0.911" ORDER="12" STRING="國遠く、 我におぢつゝ 汝が住みてありと思ふ" />
      <LINE TYPE="本文" X="1446" Y="757" WIDTH="51" HEIGHT="514" CONF="0.854" ORDER="13" STRING="悔いにけるかも 時" />
      <LINE TYPE="本文" X="1267" Y="756" WIDTH="72" HEIGHT="1222" CONF="0.793" ORDER="18" STRING="何ごとも、 完にをはりぬ。 息づきて 全く霽け" />
      <LINE TYPE="本文" X="1183" Y="758" WIDTH="45" HEIGHT="336" CONF="0.842" ORDER="19" STRING="む心ともがな" />
      <LINE TYPE="本文" X="1004" Y="753" WIDTH="59" HEIGHT="1211" CONF="0.886" ORDER="23" STRING="寛恕なき我ならめや。 ユルシ 汝を瞻るに、 心ほと〓〓" />
      <LINE TYPE="本文" X="919" Y="759" WIDTH="43" HEIGHT="342" CONF="0.846" ORDER="24" STRING="息づくころぞ" />
      <LINE TYPE="本文" X="754" Y="760" WIDTH="64" HEIGHT="1223" CONF="0.902" ORDER="27" STRING="庭の木の古葉掃きつゝ、 待ちごゝろ失せにし今" />
      <LINE TYPE="本文" X="665" Y="645" WIDTH="52" HEIGHT="568" CONF="0.847" ORDER="29" STRING="を 安しと思はむ 99" />
      <SHAPE>
        <POLYGON POINTS="667,650,667,1994,1593,1994,1593,650" />
      </SHAPE>
    </TEXTBLOCK>
