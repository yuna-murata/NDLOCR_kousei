    <TEXTBLOCK CONF="0.217">
      <LINE TYPE="本文" X="2553" Y="710" WIDTH="49" HEIGHT="1181" CONF="0.870" ORDER="0" STRING="啼き倦みて 聲やめぬらし。 鴉の止·へる木は、" />
      <LINE TYPE="本文" X="2469" Y="715" WIDTH="42" HEIGHT="387" CONF="0.813" ORDER="1" STRING="おぼろになれり" />
      <LINE TYPE="本文" X="2291" Y="714" WIDTH="45" HEIGHT="1218" CONF="0.885" ORDER="6" STRING="山の霧いや明りつゝ 鴉の 唯ひと聲は、 大き" />
      <LINE TYPE="本文" X="2210" Y="715" WIDTH="38" HEIGHT="214" CONF="0.831" ORDER="7" STRING="かりけり" />
      <LINE TYPE="本文" X="2028" Y="709" WIDTH="50" HEIGHT="1222" CONF="0.878" ORDER="9" STRING="鴉棲る梢 わかれずなりにけ り。 山の夜霧はあ" />
      <LINE TYPE="本文" X="1948" Y="714" WIDTH="42" HEIGHT="335" CONF="0.854" ORDER="10" STRING="かるけれども" />
      <SHAPE>
        <POLYGON POINTS="1950,702,1950,1933,2679,1933,2679,702" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.278">
      <LINE TYPE="本文" X="1512" Y="766" WIDTH="64" HEIGHT="1215" CONF="0.922" ORDER="13" STRING="さ夜ふけと 風はおだやむ。 麓べの澤のかや原" />
      <LINE TYPE="本文" X="1426" Y="765" WIDTH="42" HEIGHT="392" CONF="0.869" ORDER="14" STRING="そよぎつゝ聞ゆ" />
      <LINE TYPE="本文" X="1249" Y="769" WIDTH="64" HEIGHT="1219" CONF="0.891" ORDER="17" STRING="山中は 月のおも昏くなりにけり。 四方のいき" />
      <LINE TYPE="本文" X="1164" Y="766" WIDTH="52" HEIGHT="573" CONF="0.887" ORDER="18" STRING="も 絕えにけらしも の" />
      <SHAPE>
        <POLYGON POINTS="1164,759,1164,1992,1576,1992,1576,759" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.855">
      <LINE TYPE="本文" X="983" Y="770" WIDTH="72" HEIGHT="1220" CONF="0.918" ORDER="20" STRING="山深きあかとき闇や。 火をすりて、 片時見えし" />
      <LINE TYPE="本文" X="897" Y="772" WIDTH="48" HEIGHT="392" CONF="0.870" ORDER="21" STRING="わが立ち處かも" />
      <SHAPE>
        <POLYGON POINTS="897,768,897,1990,1051,1990,1051,768" />
