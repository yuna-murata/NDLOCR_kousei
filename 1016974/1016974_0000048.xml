    <TEXTBLOCK CONF="0.355">
      <LINE TYPE="本文" X="1548" Y="740" WIDTH="59" HEIGHT="1215" CONF="0.902" ORDER="20" STRING="靜けさは 常としもなし。 店とほく、 とほりて" />
      <LINE TYPE="本文" X="1462" Y="742" WIDTH="49" HEIGHT="452" CONF="0.798" ORDER="21" STRING="響く ぜに凾の音" />
      <LINE TYPE="本文" X="1289" Y="743" WIDTH="55" HEIGHT="1215" CONF="0.899" ORDER="24" STRING="さびしさに馴れつゝ住めば、 兄の子のとよもす" />
      <LINE TYPE="本文" X="1202" Y="742" WIDTH="47" HEIGHT="449" CONF="0.844" ORDER="26" STRING="家を 旅とし思ふ" />
      <LINE TYPE="本文" X="1028" Y="743" WIDTH="58" HEIGHT="1217" CONF="0.869" ORDER="28" STRING="はらからのかくむ火桶に唇かわ き、 言にあまれ" />
      <LINE TYPE="本文" X="939" Y="744" WIDTH="45" HEIGHT="386" CONF="0.823" ORDER="29" STRING="る心はたらへり" />
      <LINE TYPE="本文" X="760" Y="741" WIDTH="60" HEIGHT="1215" CONF="0.888" ORDER="31" STRING="顏ゑみて その言しぶる弟 の こゝろしたしみ" />
      <LINE TYPE="本文" X="674" Y="616" WIDTH="48" HEIGHT="573" CONF="0.801" ORDER="34" STRING="は、 我よく知れり 83" />
