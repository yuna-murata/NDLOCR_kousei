    <TEXTBLOCK CONF="0.245">
      <LINE TYPE="本文" X="2829" Y="636" WIDTH="42" HEIGHT="1338" CONF="0.871" ORDER="4" STRING="これの世は、 さびしきかもよ。 奥山も、 ひとり 18" />
      <LINE TYPE="本文" X="2744" Y="757" WIDTH="41" HEIGHT="565" CONF="0.854" ORDER="0" STRING="人住む家は さねなし" />
      <LINE TYPE="本文" X="2564" Y="666" WIDTH="48" HEIGHT="1307" CONF="0.854" ORDER="5" STRING="〓〓氣多川のさやけき見れば、 をち方のかじかの聲" />
      <LINE TYPE="本文" X="2483" Y="756" WIDTH="42" HEIGHT="510" CONF="0.804" ORDER="6" STRING="は しづけかりけり" />
      <LINE TYPE="本文" X="2303" Y="724" WIDTH="46" HEIGHT="1249" CONF="0.887" ORDER="9" STRING="ひひがほのいまださびしきいろひかも。 朝の間" />
      <LINE TYPE="本文" X="2222" Y="752" WIDTH="40" HEIGHT="629" CONF="0.842" ORDER="10" STRING="と思ふ日は 照りみてり" />
      <LINE TYPE="本文" X="2038" Y="658" WIDTH="56" HEIGHT="1311" CONF="0.783" ORDER="13" STRING="あさ茅原 つばな輝く日の光り まほにし見れ" />
      <LINE TYPE="本文" X="1963" Y="749" WIDTH="40" HEIGHT="450" CONF="0.745" ORDER="15" STRING="ば、 風そよぎけり" />
      <SHAPE>
        <POLYGON POINTS="1957,632,1957,1981,2875,1981,2875,632" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.596">
