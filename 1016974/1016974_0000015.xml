    <TEXTBLOCK CONF="0.271">
      <LINE TYPE="本文" X="2548" Y="763" WIDTH="49" HEIGHT="1221" CONF="0.912" ORDER="5" STRING="きはまりて ものさびしき時すぎて、 麥うらし" />
      <LINE TYPE="本文" X="2461" Y="762" WIDTH="43" HEIGHT="568" CONF="0.885" ORDER="6" STRING="ひとつ鳴き出でにけり" />
      <LINE TYPE="本文" X="2283" Y="756" WIDTH="50" HEIGHT="1225" CONF="0.909" ORDER="8" STRING="麥うらしの聲 ひさしくなきつげり。 ひとつと" />
      <LINE TYPE="本文" X="2198" Y="760" WIDTH="45" HEIGHT="625" CONF="0.818" ORDER="9" STRING="ころの、 をぐらくなれり" />
      <LINE TYPE="本文" X="2022" Y="756" WIDTH="47" HEIGHT="1221" CONF="0.898" ORDER="12" STRING="むぎうらし ひとつ鳴き居し聲たえて、 ふたゝ" />
      <LINE TYPE="本文" X="1935" Y="759" WIDTH="44" HEIGHT="627" CONF="0.892" ORDER="13" STRING="びは鳴かず。 山の寂けさ" />
      <SHAPE>
