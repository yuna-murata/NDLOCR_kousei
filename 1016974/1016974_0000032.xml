    <TEXTBLOCK CONF="0.333">
      <LINE TYPE="本文" X="2813" Y="628" WIDTH="43" HEIGHT="1332" CONF="0.880" ORDER="0" STRING="水のおもの深きうねりの ゆくりなく目を過ぎ 50" />
      <LINE TYPE="本文" X="2726" Y="743" WIDTH="43" HEIGHT="565" CONF="0.794" ORDER="2" STRING="ぬらし。 遠びとのかげ" />
      <LINE TYPE="本文" X="2548" Y="741" WIDTH="44" HEIGHT="1215" CONF="0.885" ORDER="5" STRING="闇夜の 雲のうごきの靜かなる 水のおもてを" />
      <LINE TYPE="本文" X="2462" Y="741" WIDTH="42" HEIGHT="390" CONF="0.835" ORDER="6" STRING="堪へて見にけり" />
      <LINE TYPE="本文" X="2289" Y="740" WIDTH="42" HEIGHT="1216" CONF="0.909" ORDER="10" STRING="みぎはに、 芥燒く人居たりけり。 靜けき夜らを" />
      <LINE TYPE="本文" X="2205" Y="800" WIDTH="41" HEIGHT="391" CONF="0.809" ORDER="14" STRING="戀ひにけるかも" />
      <LINE TYPE="本文" X="2027" Y="738" WIDTH="49" HEIGHT="1220" CONF="0.857" ORDER="19" STRING="川みづの夜はの明りに うかびたる木群のうれ" />
      <LINE TYPE="本文" X="1942" Y="742" WIDTH="43" HEIGHT="505" CONF="0.832" ORDER="17" STRING="は、 搖れ居るらしも" />
      <SHAPE>
        <POLYGON POINTS="1943,627,1943,1965,2856,1965,2856,627" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.245">
      <LINE TYPE="本文" X="1545" Y="723" WIDTH="44" HEIGHT="1237" CONF="0.886" ORDER="21" STRING="くら闇に、 そよぎ親しきも の ゝ音。 水蘆むらは、" />
      <LINE TYPE="本文" X="1459" Y="724" WIDTH="39" HEIGHT="388" CONF="0.826" ORDER="22" STRING="そがひなりけり" />
      <LINE TYPE="本文" X="1286" Y="592" WIDTH="43" HEIGHT="1292" CONF="0.897" ORDER="25" STRING="△ 遠ぞく夜風の音や。 いやさかる思ひすべなく" />
      <LINE TYPE="本文" X="1200" Y="725" WIDTH="40" HEIGHT="331" CONF="0.822" ORDER="27" STRING="雨こぼるめり" />
      <LINE TYPE="本文" X="1022" Y="725" WIDTH="47" HEIGHT="1215" CONF="0.892" ORDER="30" STRING="父母の庭の訓へにそむかねば、 心まさびしき二" />
      <LINE TYPE="本文" X="937" Y="727" WIDTH="40" HEIGHT="267" CONF="0.837" ORDER="31" STRING="十年を經つ" />
      <LINE TYPE="本文" X="758" Y="724" WIDTH="48" HEIGHT="1159" CONF="0.889" ORDER="33" STRING="川波の白くゝだくる橋柱の あらはれ來つゝ" />
      <LINE TYPE="本文" X="674" Y="607" WIDTH="43" HEIGHT="449" CONF="0.799" ORDER="35" STRING="人は還らめや 51" />
      <SHAPE>
        <POLYGON POINTS="678,589,678,1956,1590,1956,1590,589" />
      </SHAPE>
    </TEXTBLOCK>
