    <TEXTBLOCK CONF="0.941">
      <LINE TYPE="本文" X="2520" Y="983" WIDTH="47" HEIGHT="870" CONF="0.899" ORDER="5" STRING="私の姉なるその母 と、 十一二の頃から、" />
      <LINE TYPE="本文" X="2438" Y="983" WIDTH="49" HEIGHT="866" CONF="0.918" ORDER="6" STRING="私の生家に來てゐた女姪福井富美子は、" />
      <LINE TYPE="本文" X="2361" Y="980" WIDTH="46" HEIGHT="854" CONF="0.917" ORDER="7" STRING="去年女學校もすまして、 今年十九にな" />
      <LINE TYPE="本文" X="2285" Y="984" WIDTH="41" HEIGHT="457" CONF="0.919" ORDER="8" STRING="つてゐたのであつた。" />
      <SHAPE>
        <POLYGON POINTS="2288,979,2288,1855,2565,1855,2565,979" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.793">
      <LINE TYPE="本文" X="2031" Y="738" WIDTH="62" HEIGHT="1226" CONF="0.813" ORDER="9" STRING="わが家のひとり處女の、 常默すさびしきさがを モダ" />
      <LINE TYPE="本文" X="1955" Y="801" WIDTH="50" HEIGHT="393" CONF="0.641" ORDER="13" STRING="叱りけり。 わが" />
      <SHAPE>
        <POLYGON POINTS="1956,738,1956,1962,2100,1962,2100,738" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.237">
      <LINE TYPE="本文" X="1530" Y="761" WIDTH="69" HEIGHT="1226" CONF="0.897" ORDER="14" STRING="をとめはも。 肩の太りのおもりかに、 情づかず" />
      <LINE TYPE="本文" X="1444" Y="761" WIDTH="50" HEIGHT="577" CONF="0.870" ORDER="15" STRING="見えし その後姿はも" />
      <LINE TYPE="本文" X="1264" Y="763" WIDTH="71" HEIGHT="1224" CONF="0.916" ORDER="16" STRING="われの家にをとめとなりて、 糾ね髮 たけなる" />
      <LINE TYPE="本文" X="1180" Y="765" WIDTH="52" HEIGHT="636" CONF="0.884" ORDER="17" STRING="ものを 死なせつるかも" />
      <SHAPE>
        <POLYGON POINTS="1180,756,1180,1994,1601,1994,1601,756" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.772">
      <LINE TYPE="本文" X="1005" Y="769" WIDTH="71" HEIGHT="1224" CONF="0.913" ORDER="19" STRING="茨田野の水涌き濁る塚原を、 處女の家と 思ひ" />
      <LINE TYPE="本文" X="919" Y="771" WIDTH="45" HEIGHT="217" CONF="0.842" ORDER="20" STRING="堪へめや" />
      <SHAPE>
        <POLYGON POINTS="921,767,921,1996,1074,1996,1074,767" />
      </SHAPE>
    </TEXTBLOCK>
