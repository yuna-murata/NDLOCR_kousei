    <TEXTBLOCK CONF="0.859">
      <LINE TYPE="本文" X="1490" Y="726" WIDTH="57" HEIGHT="1218" CONF="0.896" ORDER="13" STRING="したに坐て ヰ もの言ふすべを知りそめて、 よき" />
      <LINE TYPE="本文" X="1404" Y="726" WIDTH="48" HEIGHT="689" CONF="0.882" ORDER="14" STRING="小をんなとなりにしものを" />
