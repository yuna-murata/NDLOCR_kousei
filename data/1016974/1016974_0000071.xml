    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.802">
      <LINE TYPE="本文" X="1228" Y="727" WIDTH="58" HEIGHT="1217" CONF="0.907" ORDER="16" STRING="朝々に 火を持ち來り、 炭つげるをさなきそぶ" />
      <LINE TYPE="本文" X="1144" Y="730" WIDTH="43" HEIGHT="447" CONF="0.833" ORDER="17" STRING="り 牀よりぞ見し" />
      <SHAPE>
        <POLYGON POINTS="1147,726,1147,1945,1283,1945,1283,726" />
