    <TEXTBLOCK CONF="0.240">
      <LINE TYPE="本文" X="2507" Y="682" WIDTH="44" HEIGHT="589" ORDER="5" STRING="く ほこりさびしも" />
      <SHAPE>
        <POLYGON POINTS="2507,682,2507,1271,2551,1271,2551,682" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.277">
      <LINE TYPE="本文" X="2564" Y="759" WIDTH="78" HEIGHT="1222" CONF="0.885" ORDER="4" STRING="人ごとのあわたゞしさよ。 閭より立ちうつり行" />
      <LINE TYPE="本文" X="2307" Y="748" WIDTH="74" HEIGHT="1219" CONF="0.883" ORDER="9" STRING="庭土に、 櫻の蕋のはらゝなり。 日なか さびし" />
      <LINE TYPE="本文" X="2248" Y="749" WIDTH="44" HEIGHT="447" CONF="0.777" ORDER="11" STRING="きあらしのとよみ" />
      <LINE TYPE="本文" X="2049" Y="734" WIDTH="72" HEIGHT="1225" CONF="0.901" ORDER="14" STRING="もの言ひの いきどほろしき隣びとの家うごく" />
      <LINE TYPE="本文" X="1980" Y="735" WIDTH="57" HEIGHT="572" CONF="0.808" ORDER="15" STRING="もよ。 あらしに見れば" />
      <SHAPE>
        <POLYGON POINTS="1986,736,1986,1980,2653,1980,2653,736" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.587">
      <LINE TYPE="本文" X="1611" Y="683" WIDTH="23" HEIGHT="27" CONF="0.214" ORDER="17" STRING="◇" />
      <LINE TYPE="本文" X="1570" Y="743" WIDTH="53" HEIGHT="1220" CONF="0.883" ORDER="19" STRING="春のあらし 靜まる町の足の音を 心したしく" />
      <LINE TYPE="本文" X="1487" Y="743" WIDTH="43" HEIGHT="391" CONF="0.770" ORDER="20" STRING="聞きにけるかも" />
      <SHAPE>
        <POLYGON POINTS="1489,704,1489,1963,1628,1963,1628,704" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.342">
      <LINE TYPE="本文" X="1307" Y="682" WIDTH="56" HEIGHT="1281" CONF="0.885" ORDER="23" STRING="〓春の夜の町音聽けば、 人ごとに むつましげな" />
      <LINE TYPE="本文" X="1227" Y="747" WIDTH="41" HEIGHT="445" CONF="0.814" ORDER="24" STRING="るもの言ひにけり" />
      <LINE TYPE="本文" X="1048" Y="745" WIDTH="54" HEIGHT="1237" CONF="0.890" ORDER="27" STRING="心ひく言をきかずなりにけり。 うと〓〓しきは、" />
      <LINE TYPE="本文" X="963" Y="742" WIDTH="42" HEIGHT="394" CONF="0.791" ORDER="29" STRING="すべなきものぞ" />
      <SHAPE>
        <POLYGON POINTS="965,673,965,1984,1360,1984,1360,673" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.738">
      <LINE TYPE="本文" X="783" Y="660" WIDTH="54" HEIGHT="1302" CONF="0.858" ORDER="35" STRING="△ ひとりのみ憤りけり。 ほがらかに、 あへばすな" />
      <LINE TYPE="本文" X="702" Y="627" WIDTH="45" HEIGHT="570" CONF="0.797" ORDER="31" STRING="はち もの言ふ人 45" />
