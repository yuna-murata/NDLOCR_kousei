    <TEXTBLOCK CONF="0.331">
      <LINE TYPE="タイトル本文" X="2770" Y="917" WIDTH="60" HEIGHT="400" CONF="0.611" ORDER="3" STRING="山および海" />
      <SHAPE>
        <POLYGON POINTS="2769,611,2769,1317,2905,1317,2905,611" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.330">
      <LINE TYPE="本文" X="2588" Y="721" WIDTH="53" HEIGHT="1226" CONF="0.842" ORDER="4" STRING="速吸の門なかに、 ひとつ逢ふものに くれなゐ" />
      <LINE TYPE="本文" X="2508" Y="722" WIDTH="46" HEIGHT="516" CONF="0.822" ORDER="5" STRING="丸の 艫じるし見ゆ" />
      <LINE TYPE="本文" X="2327" Y="717" WIDTH="55" HEIGHT="1234" CONF="0.820" ORDER="8" STRING="道の邊の廣葉の蔓 けざやかに、 日の入りの後" />
      <LINE TYPE="本文" X="2248" Y="727" WIDTH="40" HEIGHT="388" CONF="0.843" ORDER="12" STRING="の土あかりはも" />
      <LINE TYPE="本文" X="2064" Y="717" WIDTH="50" HEIGHT="1227" CONF="0.852" ORDER="14" STRING="汽車の總 こゝにし迫る小松山 峰の上の聳り" />
      <LINE TYPE="本文" X="1984" Y="724" WIDTH="42" HEIGHT="390" CONF="0.844" ORDER="15" STRING="はるけくし見ゆ" />
      <SHAPE>
        <POLYGON POINTS="1987,712,1987,1953,2649,1953,2649,712" />
      </SHAPE>
    </TEXTBLOCK>
    <TEXTBLOCK CONF="0.248">
      <LINE TYPE="本文" X="1558" Y="714" WIDTH="56" HEIGHT="1221" CONF="0.894" ORDER="17" STRING="夕關けて ダ 山まさ靑なり。 肥後の奥 人吉の町" />
      <LINE TYPE="本文" X="1471" Y="717" WIDTH="45" HEIGHT="566" CONF="0.838" ORDER="18" STRING="に、 燈 の つらなめる" />
      <LINE TYPE="本文" X="1290" Y="716" WIDTH="60" HEIGHT="1222" CONF="0.866" ORDER="20" STRING="溫泉の上に、 煙かゝれる柘の枝。 空にみだるゝ ユ" />
      <LINE TYPE="本文" X="1208" Y="778" WIDTH="47" HEIGHT="335" CONF="0.831" ORDER="21" STRING="赤とんぼかも" />
      <LINE TYPE="本文" X="1033" Y="717" WIDTH="56" HEIGHT="1226" CONF="0.900" ORDER="24" STRING="遠き道したにもちつゝ、 はたごの部屋 あした" />
      <LINE TYPE="本文" X="945" Y="724" WIDTH="55" HEIGHT="626" CONF="0.836" ORDER="25" STRING="のどかに、 飯くひをはる" />
      <LINE TYPE="本文" X="767" Y="726" WIDTH="59" HEIGHT="1216" CONF="0.860" ORDER="28" STRING="この町に たゞ一人のみ知る人の 彼も見たて" />
      <LINE TYPE="本文" X="678" Y="604" WIDTH="56" HEIGHT="512" CONF="0.757" ORDER="29" STRING="船場を步く 151 ぬ" />
      <SHAPE>
        <POLYGON POINTS="691,617,691,1953,1621,1953,1621,617" />
      </SHAPE>
    </TEXTBLOCK>
