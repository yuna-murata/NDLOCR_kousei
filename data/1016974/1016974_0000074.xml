      <LINE TYPE="本文" X="2085" Y="723" WIDTH="51" HEIGHT="1220" CONF="0.865" ORDER="11" STRING="ひた落ちに、 丘根はさがれり。 夕深き眼のくだ" />
      <LINE TYPE="本文" X="2005" Y="718" WIDTH="44" HEIGHT="395" CONF="0.850" ORDER="12" STRING="り 雪の色見ゆ" />
      <SHAPE>
        <POLYGON POINTS="2005,715,2005,1958,2410,1958,2410,715" />
