    <TEXTBLOCK CONF="0.379">
      <LINE TYPE="本文" X="2887" Y="603" WIDTH="70" HEIGHT="1351" CONF="0.798" ORDER="0" STRING="さかりゐし松葉牡丹 へりにけり。 み墓さやか 158" />
      <LINE TYPE="本文" X="2816" Y="732" WIDTH="41" HEIGHT="453" CONF="0.747" ORDER="1" STRING="になりにて 寂し" />
      <LINE TYPE="本文" X="2621" Y="721" WIDTH="60" HEIGHT="1170" CONF="0.864" ORDER="4" STRING="たゞひと言 ほめくれたりと思ふ翁がことば ヲヂ" />
      <LINE TYPE="本文" X="2540" Y="719" WIDTH="55" HEIGHT="989" CONF="0.887" ORDER="6" STRING="うや〓〓しけれど、 思ひ出でず。 今は" />
      <LINE TYPE="本文" X="2358" Y="719" WIDTH="56" HEIGHT="1227" CONF="0.884" ORDER="7" STRING="おくれ來て 寺の廣間にとほる茂吉 あつさ暑" />
      <LINE TYPE="本文" X="2283" Y="718" WIDTH="43" HEIGHT="452" CONF="0.799" ORDER="9" STRING="さと 扇ならすも" />
      <LINE TYPE="本文" X="2120" Y="710" WIDTH="54" HEIGHT="1228" CONF="0.902" ORDER="10" STRING="大川のさつきの水の濁り波。 秀がしら光る。 そ" />
      <LINE TYPE="本文" X="2040" Y="715" WIDTH="48" HEIGHT="276" CONF="0.715" ORDER="11" STRING="のくづれ波" />
      <SHAPE>
        <POLYGON POINTS="2037,603,2037,1967,2951,1967,2951,603" />
      </SHAPE>
    </TEXTBLOCK>
