    <TEXTBLOCK CONF="0.355">
      <LINE TYPE="本文" X="1230" Y="726" WIDTH="52" HEIGHT="1224" CONF="0.897" ORDER="16" STRING="ま晝の照りきはまりに 白む日の、 大地あかる" />
      <LINE TYPE="本文" X="1146" Y="728" WIDTH="43" HEIGHT="456" CONF="0.825" ORDER="18" STRING="く 月夜のごとし" />
      <LINE TYPE="本文" X="969" Y="728" WIDTH="52" HEIGHT="1227" CONF="0.862" ORDER="21" STRING="ま晝の照りみなぎらふ道なかに、 ひそかに 會" />
      <LINE TYPE="本文" X="884" Y="731" WIDTH="46" HEIGHT="510" CONF="0.740" ORDER="25" STRING="ひて、 いきづき瞻る" />
      <SHAPE>
        <POLYGON POINTS="889,722,889,1959,1281,1959,1281,722" />
      </SHAPE>
    </TEXTBLOCK>
